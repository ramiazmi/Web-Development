<div id="h5_div">
  <% ac_t5=Assessment.where("proposal_id=? and user_id=? and mark is null and criterion_id in (17)",@@proposal_id,current_user.id).count(:criterion_id) 
    fa_t5 = ac_t5>=1 ? "fa-calendar-times-o text-danger":"fa-calendar-check-o text-success"
  %>
  <i class="fa <%= fa_t5 %> fa-lg text-success pull-left"></i>
    
</div>